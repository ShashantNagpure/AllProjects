class myapp implements Runnable{

@Override
public void run() {
	// TODO Auto-generated method stub
   perform();	
}
void perform()
{	
	for(int i=0;i<5;i++)
	{
		System.out.println("hello\t"+i+"\t"+Thread.currentThread().getName());
		Thread.sleep(100);
	}
}
}

public class ExecuteDemo1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
